{
    "role": {
        "name": "oxAuth"
    },
    "instrumentation": {
        "logging": {
            "level": "INFO"
        }
    },
    "selfDiagnostics": {
        "destination": "file",
        "level": "INFO",
        "file": {
            "path": "/opt/gluu/jetty/oxauth/logs/applicationinsights.log",
            "maxSizeMb": 5,
            "maxHistory": 1
        }
    },
    "preview": {
        "sampling": {
            "overrides": [
                {
                    "attributes": [
                        {
                            "key": "code.namespace",
                            "value": "org.gluu.service.timer.TimerJob",
                            "matchType": "strict"
                        }
                    ],
                    "percentage": 0
                }
            ]
        }
    }
}
